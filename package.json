{
  "name": "mystery",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "20 || 22"
  },
  "scripts": {
    "dev": "USE_HASH=true vite",
    "build:ci": "USE_HASH=true vite build",
    "build": "npm run lint && npm run build:ci",
    "format": "prettier --ignore-path ./.eslintignore --check 'src/**/*.{js,jsx,ts,tsx,css,scss,md,json,html}' ",
    "format:fix": "prettier --ignore-path ./.eslintignore --write 'src/**/*.{js,jsx,ts,tsx,css,scss,md,json,html}' ",
    "lint": "tsc && npm run format && npm run lint:css && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && echo CODE ALL LINTED!",
    "preview": "vite preview",
    "test": "jest ",
    "npmAudit": "npm audit -audit-level high --omit=dev",
    "createTar": "node ./utils/createArchive.js",
    "prepare": "husky install",
    "lint:css": "npm run lint:css:css && npm run lint:css:js",
    "lint:css:css": "stylelint --ignore-path ./.eslintignore './**/*.+(css|scss)'",
    "lint:css:js": "stylelint --ignore-path ./.eslintignore --config .stylelintrcJS './**/*.+(ts|tsx|js|jsx)'",
    "checkCoverage": "node ./utils/testCoverage.js",
    "uglify": "uglifyjs ",
    "cypress:demo:dev": "USE_DEMO=true USE_HASH=false USE_MOCK=false vite",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "react": "^18.3.0",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.63.0",
    "react-router": "^7.9.2",
    "react-router-dom": "^7.9.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.5.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "axe-core": "^4.10.3",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.2.0",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "stylelint": "^16.24.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^29.4.4",
    "ts-jest-mock-import-meta": "^1.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "uglify-js": "^3.19.3",
    "vite": "^7.1.7"
  }
}
